package myio;

import java.io.*;
import java.util.List;

public class MyFile {

    /**
     * Ghi danh sách đối tượng vào file nhị phân
     *
     * @param filename tên của file cần ghi dữ liệu
     * @param objects  danh sách đối tượng cần ghi vào file
     * @throws IOException nếu có lỗi khi ghi vào file
     */
    public static void binaryOutputFile(String filename, List<Object> objects) throws IOException {
        try (ObjectOutputStream oos = new ObjectOutputStream(new FileOutputStream(filename))) {
            for (Object obj : objects) {
                oos.writeObject(obj);
            }
        }
    }

    /**
     * Đọc dữ liệu từ file nhị phân
     * 
     * @param filename tên của file cần đọc dữ liệu
     * @throws IOException
     */
    public static void binaryInputFile(String filename) throws IOException {
        try (FileInputStream inf = new FileInputStream(filename);
             DataInputStream in = new DataInputStream(inf)) {

            
        }
    }
}
